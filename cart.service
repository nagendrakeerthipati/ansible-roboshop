[Unit]
Description=Cart Service

[Service]
User=roboshop
Environment=REDIS_HOST=redis.sh.nagendrablog.site
Environment=CATALOGUE_HOST=catalogue.sh.nagendrablog.site
Environment=CATALOGUE_PORT=8080
ExecStart=/bin/node /app/server.js
SyslogIdentifier=cart
Restart=always

[Install]
WantedBy=multi-user.target
